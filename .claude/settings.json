{
    "autoApproveCommands": [
        "git status",
        "git log*",
        "git diff*",
        "git add*",
        "git commit*",
        "ls*",
        "cat*",
        "pwd",
        "tree*"
    ],
    "autoApproveTools": [
        "read_file",
        "list_directory",
        "search_files"
    ],
    "blockedPaths": [
        "/etc/*",
        "/usr/bin/*",
        "/usr/sbin/*",
        "/bin/*",
        "/sbin/*",
        "/System/*",
        "/Library/*",
        "/var/db/*",
        "/var/root/*",
        "/private/etc/*",
        "/private/var/db/*",
        "*.plist",
        "/boot/*",
        "/root/*"
    ],
    "blockedCommands": [
        "sudo*",
        "rm -rf /*",
        "chmod 777*",
        "chown*",
        "mkfs*",
        "dd if=*",
        ":(){ :|:& };:",
        "mv * /*"
    ]
}